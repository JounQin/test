    "vue-tsc": "^0.3.0"
  },
  "resolutions": {
    "prettier": "^2.4.1"
  },
  "commitlint": {
    "extends": "@1stg/commitlint-config"
  },
  "eslintConfig": {
    "extends": "@1stg",
    "overrides": [
      {
        "files": "*.vue",
        "rules": {
          "@typescript-eslint/no-unused-vars": 0,
          "sonar/unused-import": 0,
